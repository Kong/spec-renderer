{
  "name": "@kong/spec-renderer-dev",
  "version": "1.51.0",
  "description": "Kong's open-source spec renderer",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/kong-spec-renderer.umd.js",
  "module": "./dist/kong-spec-renderer.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "typings": "dist/types",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/kong-spec-renderer.es.js",
      "require": "./dist/kong-spec-renderer.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cross-env USE_SANDBOX=true vite",
    "build": "pnpm run typecheck && pnpm run build:package && pnpm run build:types && pnpm run build:aliases",
    "build:package": "vite build -m production",
    "build:types": "vue-tsc -p './tsconfig.build.json' --emitDeclarationOnly",
    "build:aliases": "tsc-alias -p './tsconfig.build.json'",
    "build:analyzer": "BUILD_VISUALIZER='true' vite build -m production",
    "build:sandbox": "cross-env USE_SANDBOX=true vite build -m production",
    "preview": "pnpm run build:sandbox && cross-env USE_SANDBOX=true vite preview",
    "typecheck": "vue-tsc -p './tsconfig.build.json' --noEmit",
    "stylelint": "stylelint --allow-empty-input './src/**/*.{css,scss,vue}'",
    "stylelint:fix": "stylelint --allow-empty-input './src/**/*.{css,scss,sass,vue}' --fix",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "cross-env FORCE_COLOR=1 vitest run",
    "test:open": "cross-env FORCE_COLOR=1 vitest --ui",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "peerDependencies": {
    "vue": ">= 3.3.13 < 4"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.6.4",
    "@floating-ui/vue": "^1.1.1",
    "@kong/icons": "^1.15.1",
    "@scalar/openapi-parser": "^0.7.2",
    "@seriousme/openapi-schema-validator": "^2.2.1",
    "@stoplight/http-spec": "^7.1.0",
    "@stoplight/json": "^3.21.3",
    "@stoplight/types": "^14.1.1",
    "@stoplight/yaml": "^4.3.0",
    "@vueuse/components": "^10.11.0",
    "@vueuse/core": "^10.11.0",
    "allof-merge": "^0.6.6",
    "httpsnippet-lite": "^3.0.5",
    "json-schema": "^0.4.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "oas-normalize": "^11.1.1",
    "openapi3-ts": "4.3.3",
    "sanitize-html": "^2.13.0",
    "shiki": "^1.10.3"
  },
  "resolutions": {
    "openapi3-ts": "4.3.3"
  },
  "devDependencies": {
    "@babel/types": "^7.24.9",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@evilmartians/lefthook": "^1.7.4",
    "@kong/design-tokens": "^1.15.3",
    "@kong/eslint-config-kong-ui": "1.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/stylelint-plugin": "^2.1.2",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.14.11",
    "@types/sanitize-html": "^2.11.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/ui": "^2.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "c8": "^10.1.2",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.7.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "postcss-html": "^1.7.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "semantic-release": "^24.0.0",
    "stylelint": "^16.7.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vitest": "2.0.3",
    "vue": "^3.4.33",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kong/spec-renderer.git"
  },
  "homepage": "https://github.com/Kong/spec-renderer",
  "bugs": {
    "url": "https://github.com/Kong/spec-renderer/issues"
  },
  "author": {
    "name": "Kong Inc.",
    "url": "https://konghq.com/"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "volta": {
    "node": "20.15.1",
    "pnpm": "9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true,
      "jiraLocation": "post-description",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  }
}
